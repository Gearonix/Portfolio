{
  "extends": [
    "google"
  ],
  "rules": {
    "max-len": "off",
    "no-tabs": "off",
    "require-jsdoc": "off",
    "react-hooks/exhaustive-deps": "off",
    "new-cap": "off",
    "no-empty-function": "off",
    "semi": [
      "error",
      "never"
    ],
    "object-curly-spacing": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "no-trailing-spaces": "off",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": [],
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest"
  }
}
